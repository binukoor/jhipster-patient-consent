                                 referencedTableName="scope_mast"/>

        <addForeignKeyConstraint baseColumnNames="created_by_id"
                                 baseTableName="consent"
                                 constraintName="fk_consent_created_by_id"
                                 referencedColumnNames="id"
                                 referencedTableName="person_mast"/>

    </changeSet>
</databaseChangeLog>
